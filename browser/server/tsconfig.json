{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "jsx": "react",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "resolveJsonModule": true,
      "lib": [
        "dom",
        "webworker",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "moduleResolution": "node",
      "isolatedModules": true,
      "noImplicitAny": true,
      "noEmit": true,
      "baseUrl": "."
    }
  }
  